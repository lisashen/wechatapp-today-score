/**app.wxss**/
page {
  background-color: #deecff;
  color: rgba(0, 0, 0, 0.61);
  font-size: 16px;
}
button {
  color: rgba(0, 0, 0, 0.61);
}
radio {
  transform: scale(0.5);
}
.ok-cancel-button {
  display: flex;
  justify-content: space-around;
  margin: 10px 0;
}
picker {
  background-color: rgba(0, 0, 0, 0.1);
  border-radius: 5px;
  padding: 5px;
  margin: 5px 0px 5px 5px;
  display: inline-block;
  width: 30%;
}
.add-input {
  display: inline-block;
  width: 50%;
  background-color: rgba(0, 0, 0, 0.1);
  border-radius: 5px;
  padding: 5px;
  margin: 5px 5px 5px 0;
}
.container {
  display: flex;
  flex-direction: column;
  box-sizing: border-box;
  height: 100%;
}
.label {
  height: 24px;
  line-height: 24px;
  border-radius: 12px;
  border: 1px solid #5f9bee;
  color: #5f9bee;
  display: inline-block;
  padding: 0 12px;
  margin: 5px;
}
.label-checked {
  background-color: #5f9bee;
  color: white;
}
@font-face {
  font-family: 'iconfont';
  /* project id 1057034 */
  src: url('//at.alicdn.com/t/font_1057034_jy5qgjjvj69.eot');
  src: url('//at.alicdn.com/t/font_1057034_jy5qgjjvj69.eot?#iefix') format('embedded-opentype'), url('//at.alicdn.com/t/font_1057034_jy5qgjjvj69.woff2') format('woff2'), url('//at.alicdn.com/t/font_1057034_jy5qgjjvj69.woff') format('woff'), url('//at.alicdn.com/t/font_1057034_jy5qgjjvj69.ttf') format('truetype'), url('//at.alicdn.com/t/font_1057034_jy5qgjjvj69.svg#iconfont') format('svg');
}
.iconfont {
  font-family: "iconfont" !important;
  font-size: 16px;
  font-style: normal;
  -webkit-font-smoothing: antialiased;
  -webkit-text-stroke-width: 0.2px;
  -moz-osx-font-smoothing: grayscale;
}
